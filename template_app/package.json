{
  "name": "uatp_template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "pnpm run test:prep && pnpm run test:all && pnpm run test:rust",
    "test:rust": "cd ../rust_backend && cargo test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:prep": "pnpm exec playwright install && npm run wasm",
    "test:all": "npm run test:integration && npm run test:unit-run",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "test:unit-run": "vitest run",
    "wasm": "wasm-pack build --dev --target web ../rust_backend",
    "wasm-release": "wasm-pack build --release --target web ../rust_backend"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@types/geojson": "^7946.0.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-wasm-pack": "0.1.11",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@cubejs-client/core": "^0.35.23",
    "chart.js": "^4.4.3",
    "chartjs-plugin-zoom": "^2.0.1",
    "comlink": "^4.4.1",
    "maplibre-gl": "^4.3.1",
    "svelte-maplibre": "git+https://github.com/dabreegster/svelte-maplibre.git#svelte5",
    "ua-components": "workspace:*"
  },
  "type": "module"
}
